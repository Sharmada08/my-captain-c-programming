#include<stdio.h>
void main()
{
    int n=0;
    printf("enter the value of n");
    scanf("%d",&n);
    switch(n)
  {  case 1: printf("Pizza Rs 239");
     break;
     case 2: printf("Burger Rs 129");
     break;
     case 3: printf("Pasta Rs 179");
     break;
     case 4: printf("Freanch Fries Rs 99");
     break;
     case 5: printf("Sandwich Rs 149");
     break;
     default: printf("None");
}
}
